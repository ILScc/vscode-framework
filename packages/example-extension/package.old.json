{
    "name": "example-extension",
    "version": "0.0.1",
    "displayName": "Example Extension",
    "description": "fl",
    "publisher": "author",
    "private": true,
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "sayHello",
                "title": "Say Hello to Git"
            },
            {
                "command": "writeGoodbye",
                "title": "Write Goodbye",
                "category": "Example Extras"
            }
        ]
    },
    "dependencies": {
        "@hediet/node-reload": "^0.7.3"
    },
    "devDependencies": {
        "@types/vscode": "^1.59.0",
        "vscode-framework": "workspace:*"
    },
    "main": "./out/extensionBootstrap.js",
    "engines": {
        "vscode": "^1.59.0"
    },
    "scripts": {
        "postinstall": "vscode-framework generate-types --use-script-path",
        "start": "vscode-framework start --launch-only --path ."
    }
}
