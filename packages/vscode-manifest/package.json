{
    "name": "vscode-manifest",
    "version": "0.0.4",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "files": [
        "build"
    ],
    "description": "Contains TypeScript types, JSON schema and compiled validator for VSCode Extension Manifest",
    "scripts": {
        "postinstall": "path-exists src/generated/validate.js || pnpm generate-validator",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "generate-validator": "ts-node scripts/generateSchemaValidator.ts"
    },
    "keywords": [
        "vscode",
        "extracted",
        "package.json",
        "extensions",
        "manifest",
        "read",
        "parse",
        "validate",
        "validator"
    ],
    "devDependencies": {
        "ajv": "^8.6.3",
        "chalk": "^4.1.2",
        "esbuild": "^0.13.5",
        "fs-extra": "^10.0.0",
        "jsonfile": "^6.1.0",
        "path-exists-cli": "^2.0.0",
        "pinst": "^2.1.6",
        "ts-json-schema-generator": "^0.95.0",
        "typescript-json-schema": "^0.51.0"
    },
    "dependencies": {
        "change-case": "^4.1.2",
        "type-fest": "2.3.4",
        "json-schema-typed": "^7.0.3",
        "lodash": "^4.17.21"
    },
    "repository": {
        "url": "https://github.com/zardoy/vscode-framework.git",
        "directory": "packages/vscode-manifest"
    }
}